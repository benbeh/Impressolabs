<?xml version="1.0" encoding="UTF-8"?>
<base:MenuContainerPage xmlns:base="clr-namespace:BaseMvvmToolkit;assembly=BaseMvvmToolkit"
                        xmlns="http://xamarin.com/schemas/2014/forms"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        x:Class="ImpressoApp.Pages.Profile.EditProfilePage"
                        xmlns:sharedViews="clr-namespace:ImpressoApp.SharedViews">
    <ContentPage.ToolbarItems>
        <ToolbarItem Name="saveButton" Text="Save" Priority="0" Command="{Binding SaveCommand}"/>
    </ContentPage.ToolbarItems>
    <ContentPage.Content>
        <sharedViews:ProfileEditView/>
    </ContentPage.Content>
</base:MenuContainerPage>
